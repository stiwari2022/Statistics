import time

def Execution_Time(Trials):
  def Mean(Lisp):
    Total=0
    C=0
    LLisp=len(Lisp)
    while (C<LLisp):
      Total=Total+Lisp[C]
      C=C+1
    return (Total/LLisp)
  Lisp=[]
  C=0
  while (C<Trials):
    Start=time.process_time()
    # Start
    sum_x = 0
    for i in range(1000000):
      sum_x += i
    # End
    End=time.process_time()
    Time=End-Start
    Lisp.append(Time)
    C=C+1
  return Mean(Lisp)
